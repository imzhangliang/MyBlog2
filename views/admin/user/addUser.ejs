<body>

    <div class="container" style="padding-top: 50px;">
        <form class="form" id="addUserForm">
            <div class="form-group row">
                <label for="title" class="col-sm-2">标题</label>
                <div class="col-sm-10">
                    <input type="text" name="title" class="form-control col-sm-10" />
                </div>
            </div>
            <div class="form-group row">
                <label for="content" class="col-sm-2">内容</label>
                <div class="col-sm-10">
                    <textarea name="content" class="form-control" rows="10"></textarea>
                </div>
            </div>
            <div class="form-group row">
                <div class="col-sm-2"></div>
                <div class="col-sm-10">
                    <button id="submit" type="submit" class="btn btn-default">提交</button>
                </div>
            </div>
        </form>
    </div>

    <!-- Bootstrap core JavaScript -->
    <script src="/js/libs/popper.min.js"></script>
    <script src="/js/libs/jquery.min.js"></script>
    <script src="/js/libs/bootstrap.min.js"></script>
